{
  "name": "reactjs-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --watchAll=false --coverage",
    "lint": "npm run stylelint && npm run eslint && npm run prettier:check",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx src/ --color",
    "eslint:fix": "eslint --ext .js,.jsx, src/ --color --fix",
    "stylelint": "stylelint src/**/*.scss",
    "stylelint:fix": "stylelint src/**/*.scss --fix",
    "precommit": "lint-staged",
    "prettier:check": "prettier --check src/**/*.{js,jsx,json}",
    "prettier:fix": "prettier --write src/**/*.{js,jsx,json}",
    "husky:init": "husky install && husky add .husky\\pre-commit \"npm run precommit\""
  },
  "dependencies": {
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.0",
    "@mui/material": "^5.2.2",
    "@reduxjs/toolkit": "^1.6.2",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "lint-staged": "^12.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "eslint-plugin-import": "^2.25.3",
    "husky": "^7.0.4",
    "node-sass": "^6.0.1",
    "postcss-scss": "^4.0.2",
    "prettier": "^2.5.0",
    "stylelint": "^14.1.0",
    "stylelint-config-standard": "^24.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "coverageThreshold": {
    "global": {
      "functions": 80,
      "lines": 80,
      "statements": 80,
      "branches": 80
    }
  },
  "collectCoverageFrom": [
    "src/**/*.{js,jsx}",
    "!**/__mocks__/**",
    "!**/__tests__/**",
    "!**/__snapshots__/**",
    "!**/constants/**",
    "!src/index.jsx",
    "!src/api/setup.js",
    "!src/store/configureStore.js"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.scss": [
      "stylelint --fix"
    ]
  }
}
